<?xml version="1.0" encoding="utf-8"?>

<odoo>

	<record id="viin_product_attribute_value_view_tree"
		model="ir.ui.view">
		<field name="name">viin.product.attribute.value.tree</field>
		<field name="model">viin.product.attribute.value</field>
		<field name="arch" type="xml">
			<tree string="Attribute value Tree">
				<field name="name" />
			</tree>
		</field>
	</record>

	<record id="viin_product_attribute_value_view_form"
		model="ir.ui.view">
		<field name="name">viin.product.attribute.value.form</field>
		<field name="model">viin.product.attribute.value</field>
		<field name="arch" type="xml">
			<form string="Attribute value Form">
				<sheet>
					<field name="name" />
				</sheet>
			</form>
		</field>
	</record>

</odoo>